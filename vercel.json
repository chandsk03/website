{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "main.py",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ]
}
